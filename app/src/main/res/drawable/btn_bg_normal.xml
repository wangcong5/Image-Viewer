<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--圆角弧度-->
    <corners android:radius="5dp" />

    <!--填充色-->
    <solid android:color="#1E90FF" />

    <!--矩形边框的宽度，每段虚线的长度，和两端虚线间的颜色-->
    <stroke
        android:width="1dp"
        android:color="#F7F7F7"
        android:dashGap="250dp"
        android:dashWidth="8dp" />


</shape>
